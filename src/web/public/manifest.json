{
  "name": "AI-Enhanced Group Chat",
  "short_name": "AI Chat",
  "description": "Intelligent group chat platform with AI agent assistance for enhanced communication",
  "start_url": "/",
  "display": "standalone",
  "orientation": "any",
  "theme_color": {
    "light": "#ffffff",
    "dark": "#1a1a1a"
  },
  "background_color": {
    "light": "#ffffff",
    "dark": "#1a1a1a"
  },
  "scope": "/",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon",
      "purpose": "any"
    },
    {
      "src": "icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "categories": [
    "communication",
    "productivity",
    "social",
    "ai",
    "collaboration"
  ],
  "prefer_related_applications": false,
  "related_applications": [],
  "shortcuts": [
    {
      "name": "New Chat",
      "short_name": "New Chat", 
      "description": "Start a new group chat",
      "url": "/chat/new",
      "icons": [
        {
          "src": "icon-192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Recent Chats",
      "short_name": "Recent",
      "description": "View recent conversations",
      "url": "/chat/recent",
      "icons": [
        {
          "src": "icon-192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "AI Agents",
      "short_name": "Agents",
      "description": "Manage AI assistants",
      "url": "/agents",
      "icons": [
        {
          "src": "icon-192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "screenshot-chat.png",
      "sizes": "1280x720",
      "type": "image/png",
      "platform": "wide",
      "label": "Chat Interface"
    },
    {
      "src": "screenshot-mobile.png",
      "sizes": "750x1334",
      "type": "image/png",
      "platform": "narrow",
      "label": "Mobile Interface"
    }
  ],
  "display_override": [
    "standalone",
    "minimal-ui",
    "browser"
  ],
  "lang": "en",
  "dir": "ltr",
  "id": "ai-enhanced-group-chat",
  "iarc_rating_id": "",
  "edge_side_panel": {
    "preferred_width": 480
  },
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": ["navigate-existing", "auto"]
  },
  "protocol_handlers": [
    {
      "protocol": "web+aichat",
      "url": "/chat/%s"
    }
  ]
}